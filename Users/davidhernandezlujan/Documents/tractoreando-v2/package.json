{
  "name": "tractoreando-maintenance-v2",
  "version": "2.0.0",
  "description": "Sistema avanzado de gestión de mantenimiento de vehículos multiempresa con IA, IoT y funcionalidades innovadoras",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd frontend && npm start",
    "server": "nodemon server.js",
    "build": "cd frontend && npm run build",
    "build:prod": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "clean": "rm -rf node_modules frontend/node_modules frontend/build",
    "deploy:install": "./install.sh",
    "deploy:install-standalone": "./install-standalone.sh",
    "deploy:update": "./update.sh git",

    "deploy:configure-proxy": "./configure-proxy.sh",
    "deploy:network-test": "curl -s http://localhost/health && echo 'Local OK' || echo 'Local FAIL'",
    "clean:git": "./clean-for-git.sh",
    "heroku-postbuild": "cd frontend && npm install && npm run build"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "maintenance",
    "vehicles",
    "fleet",
    "management",
    "ai",
    "iot",
    "predictive",
    "analytics",
    "sustainability",
    "blockchain"
  ],
  "author": "David Hernandez",
  "license": "MIT"
}
